apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx-proxy-pod-{{ ns_name }}
  labels:
    env: nginx-{{ ns_name }}
spec:
  replicas: 1
  selector:
    matchLabels:
      app: nginx-{{ ns_name }}
  template:
    metadata:
      labels:
        app: nginx-{{ ns_name }}
    spec:
      containers:
      - name:  nginx-proxy
        image: 063523468743.dkr.ecr.us-east-2.amazonaws.com/pfm-custom-nginx:latest
        ports:
        - containerPort: 80
